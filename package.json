{
  "name": "key-mapper",
  "productName": "key-mapper",
  "version": "1.0.0",
  "description": "Cross-platform key mapper",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "cross-env NODE_ENV=production webpack -p",
    "rebuild": "electron-rebuild --version=1.7.9",
    "package": "electron-packager . key-mapper",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=release-builds"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron": "~1.7.8",
    "electron-packager": "^10.1.0",
    "electron-rebuild": "^1.6.0",
    "file-loader": "^1.1.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "deep-equal": "^1.0.1",
    "electron-settings": "^3.1.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-toggle-switch": "^3.0.3",
    "robotjs": "^0.4.7",
    "styled-components": "^2.2.4"
  }
}
